{
  "name": "timetable",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "build:apache": "run-p type-check build-only-apache",
    "preview": "vite preview --port 4173",
    "test:unit": "vitest --environment jsdom",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:4173/make-times-forward 'cypress open --e2e'",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:4173/make-times-forward 'cypress run --e2e'",
    "build-only": "vite build",
    "build-only-apache": "vite build --mode apache",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "dayjs": "^1.10.7",
    "file-saver": "^2.0.5",
    "pinia": "^2.0.14",
    "vue": "^3.2.36",
    "vue-final-modal": "^3.4.4",
    "vue-router": "^4.0.15",
    "vue3-text-clamp": "^0.1.1"
  },
  "devDependencies": {
    "@pinia/testing": "^0.0.14",
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/file-saver": "^2.0.5",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^18.0.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.0.2",
    "@vue/tsconfig": "^0.1.3",
    "cypress": "^13.1.0",
    "eslint": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.5.1",
    "jsdom": "^20.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "start-server-and-test": "^2.0.3",
    "typescript": "~4.8.3",
    "vite": "^6.2.5",
    "vite-plugin-pwa": "^0.21.1",
    "vite-plugin-static-copy": "^2.3.0",
    "vitest": "^3.0.8",
    "vue-tsc": "^0.40.13",
    "workbox-precaching": "^6.5.4"
  }
}
